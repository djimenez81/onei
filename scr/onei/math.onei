# Math module with necessary math functionaity.

function powmod:

    input:
        integer x;
        integer n;
        integer m;
    output:
        integer y <- 1;
    end

    if n == 0:
        y = 1;
    else:
        for k in range(n):
            y = y * x;
            y = y - (y / m) * m;
        end
    end

end
